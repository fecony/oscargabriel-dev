{
	"name": "oscar-gabriel-dev",
	"version": "1.0.0",
	"description": "Oscar Gabriel",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"clean": "rm -rf ./node_modules/.vite",
		"dev": "bun alchemy dev --env-file .env.dev",
		"dev:clean": "bun clean:vite && bun dev",
		"dev:vite": "bun --env-file=.env.dev vite dev",
		"check": "biome check --write src --verbose",
		"check:unsafe": "biome check --write src --unsafe --verbose",
		"typecheck": "tsc --noEmit",
		"infra:up": "bun alchemy deploy --env-file .env.prod",
		"infra:down": "bun alchemy destroy --env-file .env.prod"
	},
	"dependencies": {
		"@biomejs/biome": "2.1.4",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.539.0",
		"radix-ui": "^1.4.2",
		"rwsdk": "0.1.35",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11",
		"vite": "6.3.5"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250813.0",
		"@content-collections/core": "^0.10.0",
		"@content-collections/markdown": "^0.1.4",
		"@content-collections/vite": "^0.2.6",
		"@types/node": "^24.2.1",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"alchemy": "^0.60.2",
		"tw-animate-css": "^1.3.6",
		"typescript": "^5.9.2",
		"wrangler": "^4.29.0",
		"zod": "^4.0.17"
	},
	"packageManager": "bun@1.2.20",
	"trustedDependencies": [
		"@swc/core",
		"@tailwindcss/oxide",
		"esbuild",
		"sharp",
		"workerd"
	]
}
