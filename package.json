{
	"name": "oscar-gabriel-dev",
	"version": "1.0.0",
	"description": "Oscar Gabriel",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"clean": "rm -rf ./node_modules/.vite",
		"dev": "bun clean && bun vite dev",
		"dev:watch": "bun clean && rwsync --watch \"npm run dev\"",
		"check": "biome check --write src --verbose",
		"check:unsafe": "biome check --write src --unsafe --verbose",
		"infra:destroy": "bun alchemy.run.ts destroy",
		"infra:up": "bun alchemy.run.ts up",
		"preview": "bun build && bun vite preview",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@biomejs/biome": "2.1.3",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.534.0",
		"radix-ui": "^1.4.2",
		"rwsdk": "^0.1.27",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11",
		"vite": "6.3.5"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250731.0",
		"@content-collections/core": "^0.10.0",
		"@content-collections/markdown": "^0.1.4",
		"@content-collections/vite": "^0.2.6",
		"@types/node": "^24.1.0",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"alchemy": "^0.55.3",
		"tw-animate-css": "^1.3.6",
		"typescript": "^5.8.3",
		"wrangler": "^4.26.1",
		"zod": "^4.0.14"
	},
	"packageManager": "bun@1.2.18",
	"trustedDependencies": [
		"@swc/core",
		"@tailwindcss/oxide",
		"esbuild",
		"sharp",
		"workerd"
	]
}
